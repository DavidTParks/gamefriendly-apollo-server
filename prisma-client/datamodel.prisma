type User {
  id: ID! @id @unique
  createdAt: DateTime! @createdAt
  email: String! @unique
  name: String!
  password: String!
  gameSessions: [GameSession!]!
}

type GameSession {
  id: ID! @id @unique
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  title: String!
  active: Boolean!
  user: User
}